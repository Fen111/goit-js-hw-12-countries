{{#each this}}

<h1>{{name}}</h1>
<div class="info">
    <div>
        <h2>Capital: {{capital}}
            <h2 />
            <h2>Population:{{population}}</h2>
            <h2>Languages:</h2>
            <ul>
                {{#each languages}}
                <li>{{this.name}}</li>
                {{/each}}
            </ul>
    </div>
    <div><img src={{flag}} alt="" class="flag" /></div>
</div>`
{{/each}}